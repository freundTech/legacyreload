{
  "manifest_version": 2,
  "name": "Legacy Reload",
  "version": "0.1",
  "description": "Brings back the reload button in the url bar",
  "icons": {
    "48": "icons/refresh.svg",
    "96": "icons/refresh.svg"
  },
  "page_action": {
    "browser_style": true,
    "default_icon": {
      "19": "icons/refresh.svg",
      "38": "icons/refresh.svg",
      "48": "icons/refresh.svg"
    }
  },
  "permissions": ["webNavigation", "tabs"],
  "background": {
    "scripts": [
      "legacyreload.js"
    ]
  }
}